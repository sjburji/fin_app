<div class="activity_links">
  <%= link_to 'Back to Dashboard', dashboard_path %>
</div>
<br/>

<h3>Listing Stores</h3>

<table>
  <tr>
    <th>Name</th>
    <th>Franchisee</th>
    <th>Parent</th>
    <th>Street1</th>
    <th>Street2</th>
    <th>Pincode</th>
    <th>City</th>
    <th>State</th>
    <th>Country</th>
    <th>Landline</th>
    <th>Mobile</th>
    <th>Facsimile</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stores.each do |store| %>
  <tr>
    <td><%= store.name %></td>
    <td><%= store.franchisee_id %></td>
    <td><%= store.parent_id %></td>    
    <td><%= store.street1 %></td>
    <td><%= store.street2 %></td>
    <td><%= store.pincode %></td>
    <td><%= store.city %></td>
    <td><%= store.state %></td>
    <td><%= store.country %></td>
    <td><%= store.landline %></td>
    <td><%= store.mobile %></td>
    <td><%= store.facsimile %></td>
    <td><%= store.email %></td>
    <td><%= link_to 'Show', store %></td>
    <td><%= link_to 'Edit', edit_store_path(store) %></td>
    <td><%= link_to 'Destroy', store, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Store', new_store_path %>
